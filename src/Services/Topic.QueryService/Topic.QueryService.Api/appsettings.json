{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft": "Information",
            "System": "Information"
        },
        "Console": {
            "IncludeScopes": true,
            "LogLevel": {
                "Default": "Information",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        }
    },
    "ConnectionStrings": {
